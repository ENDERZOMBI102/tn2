"Sprite"
{
	"$baseTexture" "blegacy/effects/candleflame2"
	"$spriteorigin" "[ 0.50 0.50 ]"
	"$translucent" "1"
	"$nocull" "1"
	"$vertexcolor" "1"
	"$vertexalpha" "1"
	"$spriteorientation" "parallel_upright"
	"%keywords" "mazur"

	"Proxies"
	{
 		"AnimatedTexture" 
         	{ 
            		"animatedTextureVar" "$basetexture" 
            		"animatedTextureFrameNumVar" "$frame" 
            		"animatedTextureFrameRate" "30" 
         	}
	}

}

